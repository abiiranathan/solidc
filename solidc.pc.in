prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@

Name: solidc
Description: SolidC library is a collection of cross-platform C functions and data structures that are useful for systems programming.
Version: @PROJECT_VERSION@
Libs: -L${libdir} -lsolidc -lm -lpthread
Cflags: -I${includedir}
